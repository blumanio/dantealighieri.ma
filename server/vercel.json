{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/generated-posts",
      "dest": "index.js"
    },
    {
      "src": "/api/generated-posts/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "status": 308,
      "headers": {
        "Location": "https://studentitaly.it/$1"
      }
    }
  ]
}